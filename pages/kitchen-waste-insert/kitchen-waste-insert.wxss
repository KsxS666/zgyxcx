/* pages/kitchen-waste-insert/kitchen-waste-insert.wxss */
@import "../../common/common.wxss";
page{
  background: #ffffff;
  font-weight: bold
}
.house-tab{
  align-items: center;
  padding: 0 30rpx;
}
.community-info{
  padding: 0 30rpx;
  height: 96rpx;
  border-bottom: 1px solid #e5e5e5;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  background: #ffffff
}
.community-name{
  margin-right: 20rpx;
}
.down-arrow{
  margin-left: 10rpx;
  font-size: 24rpx;
}
.card-wrapper{
  margin-top: 30rpx;
  overflow: hidden;
}
.card-wrapper navigator,.card-wrapper view{
  margin-left: 25rpx;
  margin-bottom: 25rpx;
  float: left;
  width:210rpx;
  height: 210rpx;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  font-weight: bolder;
  position: relative;
  border-radius: 10rpx;
}
.card-wrapper navigator image,.card-wrapper view image{
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
}
.card-wrapper navigator text,.card-wrapper view text{
  z-index: 1
}
.flag0{
  background: #f5f5f5;
  color: #a6a6a6;
  padding: 2px;  
  /* border: 2px solid rgba(229,82,53, 0) */
  
}
.flag1{
  background: #f5f5f5;
  color: #8f8f8f;
  border: 2px solid #a6a6a6
}
.card-wrapper navigator.flag2{
  color: #00ac92;
  /* border: 2px solid rgba(0,172,146, 0) */
  padding: 2px
}
.card-wrapper navigator.flag3{
  color: #e55235;
  /* border: 2px solid rgba(229,82,53, 0) */
  padding: 2px
}
